import math
x = int(input("Ingresa el primer número: "))
y = int(input("Ingresa el segundo número: "))
z = int(input("Ingresa el tercer número: "))


if (x>y and y>z):
    print ("", x,"-", y, "-", z)
elif (y>x and x>z):
    print ("", y,"-", x, "-", z)
elif (z>x and x>y):
    print ("", z,"-", x, "-", y)
elif (z>y and y>x):
    print ("", z,"-", y, "-", x)
elif (x>z and z>y):
    print ("", x,"-", z, "-", y)
elif (y>z and z>x):
    print ("", y,"-", z, "-", x)
else:
    print("Se ingresaron números iguales")